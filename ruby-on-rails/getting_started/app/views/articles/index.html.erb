<h1>Your Articles: </h1>
<%= link_to "Create", new_article_path%>
<ul>
    <% @articles.each do |a| %>
    <li> 
        <h2>
            <%= a.title.capitalize %> 
        </h2>
        <p>
            <%= a.body %>
            <%= link_to "Show", article_path(a) %>
            <%= link_to "Edit", edit_article_path(a) %>
            <%= link_to "Delete", article_path(a), method: :delete %>
        </p>
    </li>
    <% end %>
</ul>