<%= form_for @article do |f| %>
  <!-- Show error if encountered -->
   render 'shared/errors'

    <!-- Render form using helper f.form -->
    <%= f.label :title %> <br>
    <%= f.text_field :title %> <br>
    <%= f.label :body %> <br>
    <%= f.text_area :body %><br>
    <%= f.submit %>
<% end %>
